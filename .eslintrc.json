{
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "semi": "error",
    "react/jsx-one-expression-per-line": "off",
    "react/destructuring-assignment": "off",
    "max-len": ["error", { "code": 80 }],
    "space-infix-ops": "error"
  },
  "parser": "babel-eslint"

}